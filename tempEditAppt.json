FUNCTION GregToHijri(ADate:TDateTime):String;
var Maxd,Year0,Month0,Day0:integer;
    y,MM,d,h,m,s,ms:Word;
    r,Day1:Double;
    //cr:String;
    CorrectDays:String[2];
    good:boolean;
BEGIN
     DecodeDate(ADate, Y, MM, D);
     DecodeTime(ADate, h, m, s,ms);

     //r:=(Y*365.2425+MM*30.436875+ D  -  621.8889322*365.2425);
     r:=Y*365.2425+MM*30.436875+D - 621.574981435*365.2425;
                                    //621.578082192
     Year0 := floor( r / 354.367056 );
             Month0:= floor((r / 29.530588) - Year0*12 );

     Maxd:=29 + (Month0 mod 2);
     if Month0=12 then
     begin
        if (Year0 mod 30) in [2, 5, 8, 10, 13, 16, 19, 21, 24, 27, 29] then
           Maxd:=Maxd+1;
     end;

     if Day0>Maxd then
        Day0:=Maxd;


     Day1:=((r / 29.530588) - Year0*12 -Month0) *29.530588;

     if Day1<0.5 then
     begin
        if Month0>1 then
        begin
           Month0:=Month0-1;
           Day0:=Maxd;
           good:=true;
        end;
     end
     Else Day0:= floor(day1 );

     if Day0<1 then
        Day0:=1;

     result:=Makestr([PadStringL(InttoStr(Day0),'0',2),PadStringL(InttoStr(Month0),'0',2),Year0],'-');
     if (h>0) or (m>0) or (s>0) then
        result:=Result+' '+MakeStr([PadStringL(InttoStr(h),'0',2),PadStringL(InttoStr(m),'0',2),PadStringL(InttoStr(s),'0',2)],':');



// 2010-12-29 14:11:44
END;




